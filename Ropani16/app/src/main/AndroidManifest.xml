<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.realestate.ropani16">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.realestate.ropani16.profileActivity"></activity>
        <activity
            android:name="com.realestate.ropani16.DetailActivity"
            android:parentActivityName="com.realestate.ropani16.Main2Activity" />
        <activity android:name="com.realestate.ropani16.Main2Activity" />
        <activity
            android:name="com.realestate.ropani16.SignupActivity"
            android:parentActivityName="com.realestate.ropani16.MainActivity" />
        <activity
            android:name="com.realestate.ropani16.LoginActivity"
            android:parentActivityName="com.realestate.ropani16.MainActivity" />
        <activity android:name="com.realestate.ropani16.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>